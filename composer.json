    "developer",
    "customizer",
    "admin",
    "backend",
    "ui"
  ],
  "homepage": "http://wordpress.org/plugins/customizer-definitely/",
  "authors": [
    {
      "name": "Felix Arntz",
      "email": "felix-arntz@leaves-and-love.net",
      "homepage": "http://leaves-and-love.net",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "felix-arntz@leaves-and-love.net",
    "issues": "https://github.com/felixarntz/customizer-definitely/issues"
  },
  "config": {
    "preferred-install": "dist"
  },
  "autoload": {
